[alias]
	co = checkout
	ec = config --global -e
	rbmaster = rebase origin/main
	squash = rebase -i HEAD~2
	squash3 = rebase -i HEAD~3
	sq = rebase -i HEAD~2
	sq3 = rebase -i HEAD~3
	fwl = push --force-with-lease
	cm = commit -am
	wcm = commit -am \"WORK IN PROGRESS COMMIT\"
	dir = config -l
	listconfig = config -l
	prune = gc --prune=now
	currentbranch = rev-parse --abbrev-ref HEAD
	rmnonsrc = clean -xdf
	undo = checkout
	masterreset = copull main
	nb = "!f() { \
			git conewbranch EIH-$1 ; \
		}; f"	
	conewbranch = "!f() { \
			git checkout main ; \
			git pull ; \
			git checkout -b $1 ; \
			git setorigin ; \
			git diff package.json ; \
		}; f"
	setorigin = "!f() { \
			currentBranch=`git currentbranch` ; \
			git push --set-upstream origin $currentBranch ; \
		}; f"
	copull = "!f() {\
			git fetch ; \
			git checkout $1 ; \
			git pull; \
		}; f"
	squashpush = "!f() {\
			git commit -m \"WORK IN PROGRESS COMMIT PLEASE REMOVE THIS\" ; \
			git squash ; \
			git fwl; \
		}; f"
	rekt = "!f() {\
			echo \"Gittin rekt\" ; \
			echo \"rm -rf *\" ; \
			echo \"Got rekt.\" ; \
			echo \"jk not rly\" ; \
		}; f"
	cleanbadref = "!f() { rm -rv .git/refs/remotes/origin/$1; }; f"
	undofile = "!f() {\
			git checkout main $1; \
		}; f"
	ls = "!f() {\
			git config -l | grep -vF branch. | sort; \
		}; f"
	deletebranch = "!f() {\
				git branch -D $1; \
				git push origin --delete $1; \
		}; f"
	updatedate = "!f() {\
			git commit --amend --date="now"; \
		}; f"
[user]
	name = Alan Delimon
	email = adelimon@gmail.com
[diff]
    tool = code
[difftool "code"]
    cmd = "code $LOCAL $REMOTE"
[difftool]
    prompt = true
[merge]
	tool = kdiff3
[mergetool "code"]
    cmd = "code $LOCAL $REMOTE"
[mergetool]
    prompt = true
[core]
	trustctime = false
